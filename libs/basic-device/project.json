{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/basic-device/src",
  "targets": {
    "build": {
      "executor": "@nx-plus/vue:library",
      "options": {
        "dest": "dist/libs/basic-device",
        "entry": "libs/basic-device/src/index.ts",
        "tsConfig": "libs/basic-device/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/basic-device/**/*.{ts,tsx,vue}"]
      }
    }
  },
  "tags": []
}
